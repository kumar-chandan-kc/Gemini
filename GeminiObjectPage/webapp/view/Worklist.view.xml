		<!-- DynamicPage Content -->
		<f:content>
			<Table id="table" width="auto" items="{ path: 'worklistView>/BOTable', sorter: { path: 'ObjectName', descending: false } }"
				noDataText="No Matching Found" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true"
				updateFinished="onUpdateFinished" selectionChange="onSelect">
				<headerToolbar>
					<Toolbar>
						<Title id="tableHeader" text="{worklistView>/worklistTableTitle}"/>
						<ToolbarSpacer/>
						<!--<Button tooltip="{i18n>ViewSettings}" icon="sap-icon://action-settings" press="handleViewSettingsDialogButtonPressed"/>-->
						<SearchField id="searchField" tooltip="Enter ObjectName, Area.. to search" search="onSearch" width="auto" liveChange="onSearch"></SearchField>
					</Toolbar>
				</headerToolbar>
				<columns>
					<Column id="ObjectName" width="230px" visible="true">
						<Text text="{i18n>ObjectNameDialogFragment}" id="nameColumnTitle"/>
					</Column>
					<Column id="Area" visible="true">
						<Text text="{i18n>AreaDialogFragment}" id="nameColumnTitle1"/>
					</Column>
					<Column id="PersonResponsible" visible="true">
						<Text text="{i18n>PersonResponsibleDialogFragment}" id="nameColumnTitle2"/>
					</Column>
					<Column id="ApplicationComponent" visible="true">
						<Text text="{i18n>ApplicationComponentDialogFragment}" id="nameColumnTitle3"/>
					</Column>
					<Column id="Status" visible="true">
						<Text text="{i18n>StatusDialogFragment}" id="nameColumnTitle5"/>
					</Column>
					<Column id="isCloudRelevant" visible="false">
						<Text text="{i18n>IsCloudRelevantDialogFragment}" id="nameColumnTitle4"/>
					</Column>
					<Column id="BadiCount" visible="false">
						<Text text="{i18n>BadiCountDialogFragment}" id="nameColumnTitle6"/>
					</Column>
					<Column id="OdataCount" visible="false">
						<Text text="{i18n>OdataCountDialogFragment}" id="nameColumnTitle7"/>
					</Column>
					<Column id="CDSCount" visible="false">
						<Text text="{i18n>CDSCountDialogFragment}" id="nameColumnTitle8"/>
					</Column>
					<Column id="Statuscomments" visible="false">
						<Text text="{i18n>StatusCommentsDialogFragment}" id="nameColumnTitle49"/>
					</Column>
					<Column id="SearchObject" visible="false">
						<Text text="{i18n>SearchObjectDialogFragment}" id="nameColumnTitle11"/>
					</Column>
					<Column id="isExtensionApplicable" visible="false">
						<Text text="{i18n>isExtensionApplicableDialogFragment}" id="nameColumnTitle12"/>
					</Column>
					<Column id="CDSViewname" visible="false">
						<Text text="{i18n>CDSViewnameDialogFragment}" id="nameColumnTitle13"/>
					</Column>
					<Column id="isSearchapplicable" visible="false">
						<Text text="{i18n>isSearchapplicableDialogFragment}" id="nameColumnTitle15"/>
					</Column>
					<Column id="isOutputapplicable" visible="false">
						<Text text="{i18n>isOutputapplicableDialogFragment}" id="nameColumnTitle14"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem type="Navigation" press="onPress">
						<cells>
							<ObjectIdentifier title="{worklistView>ObjectName}"/>
							<Text text="{worklistView>Area}"/>
							<Text text="{worklistView>PersonResponsible}"/>
							<Text text="{worklistView>ApplicationComponent}"/>
							<Text text="{worklistView>Status}"/>
							<Text text="{worklistView>isCloudRelevant}"/>
							<Text text="{worklistView>BadiCount}"/>
							<Text text="{worklistView>OdataCount}"/>
							<Text text="{worklistView>CDSCount}"/>
							<Text text="{worklistView>StatusComments}"/>
							<Text text="{worklistView>SearchObject}"/>
							<Text text="{worklistView>isExtensionApplicable}"/>
							<Text text="{worklistView>CDSViewname}"/>
							<Text text="{worklistView>isSearchapplicable}"/>
							<Text text="{worklistView>isOutputapplicable}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
